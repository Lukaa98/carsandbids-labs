{
  "manifest_version": 3,
  "name": "C&B URL + Details",
  "version": "1.2.2",
  "description": "Collect Cars & Bids listing URLs, and (optionally) fetch a few details.",
  "content_scripts": [
    {
      "matches": ["https://carsandbids.com/*", "https://www.carsandbids.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "host_permissions": ["https://carsandbids.com/*", "https://www.carsandbids.com/*"],
  "permissions": []
}
